{
  "name": "easy-notify",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "yarn run migrate && node server/lib",
    "watch": "concurrently -c \"black.bgBlue,black.bgMagenta\" -n \"client,server\" \"ng build --watch\" \"gulp watch\"",
    "build": "yarn run build-server && yarn run build-client",
    "build-server": "gulp build-server",
    "build-client": "ng build",
    "test": "yarn run test-server && yarn run test-client",
    "test-server": "jest",
    "test-client": "ng test --watch=false",
    "lint": "yarn run lint-server && yarn run lint-client",
    "lint-server": "tslint --config server/tslint.json --project server",
    "lint-client": "tslint --config client/tslint.json --project client",
    "e2e": "ng e2e",
    "migrate": "exec $(npm prefix)/.npm-scripts/db_migrate",
    "seed": "exec $(npm prefix)/.npm-scripts/db_seed",
    "snyk-protect": "snyk protect",
    "prepublish": "npm run snyk-protect",
    "heroku-postinstall": "npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cli": "~1.7.2",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/http": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "@angular/service-worker": "^5.2.7",
    "@types/gulp": "^4.0.5",
    "@types/jasmine": "~2.8.3",
    "@types/jasminewd2": "~2.0.2",
    "@types/jest": "^22.1.4",
    "@types/node": "^9.4.6",
    "@types/sequelize": "^4.27.8",
    "body-parser": "^1.18.2",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "core-js": "^2.4.1",
    "express": "^4.16.2",
    "gulp": "^4.0.0",
    "gulp-cli": "^2.0.1",
    "gulp-develop-server": "^0.5.2",
    "gulp-typescript": "^4.0.1",
    "morgan": "^1.9.0",
    "pg": "^7.4.1",
    "rxjs": "^5.5.6",
    "sequelize": "^4.35.1",
    "sequelize-cli": "^4.0.0",
    "snyk": "1.69.10",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "~2.5.3",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "codelyzer": "^4.0.1",
    "concurrently": "^3.5.1",
    "jasmine-core": "~2.8.0",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "^22.4.2",
    "karma": "~2.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.1.2",
    "ts-node": "~4.1.0",
    "tslint": "~5.9.1",
    "tslint-angular": "^1.1.1"
  },
  "engines": {
    "node": "9.3.0",
    "yarn": "1.3.2"
  },
  "jest": {
    "coverageDirectory": "<rootDir>/coverage/server",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/server/src/**.(ts|tsx|js)"
    ],
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/jest/tsPreprocessor.js"
    },
    "testMatch": [
      "<rootDir>/server/__tests__/*.(ts|tsx|js)"
    ]
  },
  "keywords": [
    "notification",
    "webhook",
    "slack"
  ],
  "snyk": true
}
